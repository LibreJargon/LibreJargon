{
    "manifest_version": 2,
    "name": "LibreJargon",
    "version": "0.0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["*://*/*.pdf"],
    "page_action": {
        "show_matches": ["*://*/*.pdf"]
    },
    "web_accessible_resources": [
        "viewer/viewer.html"
    ]
}
