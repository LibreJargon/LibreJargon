{
    "manifest_version": 2,
    "name": "LibreJargon",
    "version": "0.0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "*://*/*.pdf"],
    "browser_action": {
      "default_popup": "authentication/auth.html"
    },
    "web_accessible_resources": [
        "viewer/viewer.html",
        "readinglist/readinglist.html"
    ]
}
